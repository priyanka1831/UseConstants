package inheritance;

public interface PhysicalConstants {

	long SPEEDOFLIGHTINVACCUM=299792458L;
	double GRAVITATIONALCONSTANT=6.67428E-11;
	float STANDARDGRAVITATIONALACCELARATION=9.80665F;
	
	

}
package inheritance;
import static inheritance.PhysicalConstants.SPEEDOFLIGHTINVACCUM;
import static inheritance.PhysicalConstants.GRAVITATIONALCONSTANT;
import static inheritance.PhysicalConstants.STANDARDGRAVITATIONALACCELARATION;
public class UseConstants {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
  float M=5;
  double E=M*SPEEDOFLIGHTINVACCUM*SPEEDOFLIGHTINVACCUM;
  
  float m1=2.2f,m2=3.5f;
  float r=2.0f;
  
  double F=GRAVITATIONALCONSTANT*(m1*m2)/r*r;
  
  float t=5.25f;
  float d=(0.5f)*STANDARDGRAVITATIONALACCELARATION*t;
  
  System.out.println("E="+E);
  System.out.println("F="+F);
  System.out.println("d="+d);
  
	}

}
